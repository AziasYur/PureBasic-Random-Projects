  Text_About_2 = TextGadget(#PB_Any, 5, 60, 395, 40, "In no event shall the author be liable for any claim, damages or other liability, arising from, out of, or in connection with the software or the use or dealings in the software.")
  Text_About_3 = TextGadget(#PB_Any, 5, 115, 210, 15, "Version 1.0.0.1")
EndProcedure

Procedure Window_MD5About_Events(event)
  Select event
    Case #PB_Event_CloseWindow
      ProcedureReturn #False

    Case #PB_Event_Menu
      Select EventMenu()
      EndSelect

    Case #PB_Event_Gadget
      Select EventGadget()
        Case Button_CloseAbout
          Button_CloseAboutEvent(EventType())          
      EndSelect
  EndSelect
  ProcedureReturn #True
EndProcedure

