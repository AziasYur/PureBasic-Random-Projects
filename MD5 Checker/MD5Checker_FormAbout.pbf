Global Text_About_0, Button_CloseAbout, Text_About_1, Text_About_C0, Text_About_C1, Text_About_2, Text_About_3

Enumeration FormWindow
  #Window_MD5About
EndEnumeration

Declare Button_CloseAboutEvent(EventType)

Procedure OpenWindow_MD5About(x = 0, y = 0, width = 400, height = 210)
  OpenWindow(#Window_MD5About, x, y, width, height, "About", #PB_Window_Tool | #PB_Window_ScreenCentered)
  Text_About_0 = TextGadget(#PB_Any, 5, 10, 395, 30, "MD5 Checker Bootleg Edition is a utility program to generate and check file's MD5 Hashes.")
